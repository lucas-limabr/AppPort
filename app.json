{
  "expo": {
    "name": "Portuguito",
    "slug": "Portuguito",
    "version": "3.1.0",
    "orientation": "portrait",
    "icon": "./assets/iconePortuguito4.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Portuguito"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconePortuguito4.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.Portuguito",
      "versionCode": 3
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7cb56afa-0580-42c4-80b5-716e39b3a1ff"
      }
    },
    "owner": "lucas_lima22"
  }
}
